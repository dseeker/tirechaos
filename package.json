{
  "name": "tire-chaos",
  "version": "0.2.0",
  "description": "Physics-based tire rolling destruction game with cinematic camera system - Now powered by Babylon.js!",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\""
  },
  "dependencies": {
    "@babylonjs/core": "^6.42.0",
    "@babylonjs/materials": "^6.42.0",
    "@babylonjs/loaders": "^6.42.0",
    "@babylonjs/post-processes": "^6.42.0",
    "@babylonjs/inspector": "^6.42.0",
    "cannon-es": "^0.20.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "keywords": [
    "game",
    "physics",
    "tire",
    "destruction",
    "3d",
    "babylonjs",
    "webgl"
  ],
  "author": "TIRE CHAOS Team",
  "license": "MIT"
}
